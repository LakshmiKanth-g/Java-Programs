
//Find the Smallest Element in an Array

import java.io.*;

public class SmallestElmt {
	public static void main(String[] args) {
		
		int[] arr= {3,5,2,1,6,7,-9};
		int min=arr[0];
		int n=arr.length;
		for(int i=0;i<n;i++) {
			if(arr[i]<min) {
				min=arr[i];
			}
		}
		System.out.println(min);
		
	}
}

/*
Output: 
-9
*/
