// Find a given char within the string and the location. 

import java.util.*;

public class Main {
    public static void main(String[] args) {
      String s=new String("Hello");
      int num=s.length();
      for(int i=0;i<num;i++){
        char ch=s.charAt(i);
        if(ch=='e'){
          System.out.println("printed "+i+" "+ch);
          break;
        }
      }
  }
}

/*
Output:

printed 1 e
*/
